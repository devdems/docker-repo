<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>Flickr-uploader</Name>
  <Beta>False</Beta>
  <Category>Productivity:</Category>
  <Date>2020-03-18</Date>
  <Overview>Upload pictures to flicker</Overview>
  <Description></Description>
  <Registry>https://registry.hub.docker.com/u/snoopy86/flickr-uploader</Registry>
  <Repository>snoopy86/flickr-uploader</Repository>
  <DonateText>Buy me a beer.</DonateText>
  <DonateImg>https://www.paypalobjects.com/en_US/i/btn/btn_donateCC_LG.gif</DonateImg>
  <DonateLink>https://www.paypal.com/cgi-bin/webscr?cmd=_s-xclick&amp;hosted_button_id=FDMP56NCG5A26&amp;source=url</DonateLink>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Environment>
    <Variable>
      <Name>PYTHONUNBUFFERED</Name>
      <Value>0</Value>
    </Variable>
  </Environment>
  <Networking>
    <Mode>host</Mode>
  </Networking>
  <Data>
   <Volume>
      <HostDir></HostDir>
      <ContainerDir>/pictures</ContainerDir>
      <Mode>r</Mode>
    </Volume>
      <Volume>
      <HostDir></HostDir>
      <ContainerDir>/folders2flickr</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <WebUI></WebUI>
  <Banner>http://www.winona.edu/Images/flickr_banner-519.jpg</Banner>
  <Icon>https://raw.githubusercontent.com/snoopy86/docker-repo/master/snoopy/flickr-logo-png-8766.png</Icon>
</Container>
